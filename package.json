{
  "name": "@timeheater/penumbra-mcp",
  "version": "0.1.0",
  "description": "MCP server for interacting with Penumbra blockchain",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timeheater/penumbra-mcp.git"
  },
  "keywords": [
    "mcp",
